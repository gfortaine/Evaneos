{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb",
    "cleanjs",    
    "plugin:flowtype/recommended"    
  ],
  "plugins": [
    "flowtype",
    "jest"
  ],
  "rules" : {
    "fp/no-nil": "warn",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "semi": ["error", "never"]
  }
}